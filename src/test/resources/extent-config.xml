<extentreports>
    <configuration>
        <theme>standard</theme> <!-- or "dark" -->
        <encoding>utf-8</encoding>
        <protocol>https</protocol>
        <documentTitle>DS Algo Test Report</documentTitle>
        <reportName>DS Algo Execution Summary</reportName>
        <timestampFormat>dd-MM-yyyy HH:mm:ss</timestampFormat>
        
        <!-- Enable charts -->
        <chartVisibilityOnOpen>true</chartVisibilityOnOpen> <!-- Pie chart -->
        <timelineEnabled>true</timelineEnabled> <!-- Timeline view -->
        <offlineMode>true</offlineMode>
    </configuration>
</extentreports>